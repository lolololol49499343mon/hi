local function WAYJSP_fake_script() -- on.LocalScript 
	local script = Instance.new('LocalScript', on)

	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Visible = false
		script.Parent.Parent.off.Visible = true
	end)
end
coroutine.wrap(WAYJSP_fake_script)()
local function XTESE_fake_script() -- off.LocalScript 
	local script = Instance.new('LocalScript', off)

	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Visible = false
		script.Parent.Parent.on.Visible = true
	end)
end
coroutine.wrap(XTESE_fake_script)()
