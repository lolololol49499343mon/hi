local function XKYZEC_fake_script() -- closeextra.LocalScript 
	local script = Instance.new('LocalScript', closeextra)

	script.Parent.MouseButton1Down:connect(function()
		game.CoreGui.PartnerUI.Frame.Frame.Visible = false
		game.CoreGui.PartnerUI.Frame.extraoption.Visible = true
		script.Parent.Visible = false
	end)
end
