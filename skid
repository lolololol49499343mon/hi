TextButton.TextWrapped = true
TextButton.MouseButton1Down:connect(function()
	if Ctag.Text == "ðŸ”§Moderator" then
		local chatmessage = game.CoreGui.PartnerUI.Frame.TextBox.Text

		local args = {
			[1] = "[ðŸ”§Moderator]: " .. chatmessage,
			[2] = "All"
		}

		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))

	end
	if Ctag.Text == "ðŸ”¥Partner" then
		local chatmessage = game.CoreGui.PartnerUI.Frame.TextBox.Text

		local args = {
			[1] = "[ðŸ”¥Partner]: " .. chatmessage,
			[2] = "All"
		}

		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))

	end
	if Ctag.Text == "ðŸ’ŽNFT" then
		local chatmessage = game.CoreGui.PartnerUI.Frame.TextBox.Text

		local args = {
			[1] = "[ðŸ’ŽNFT]: " .. chatmessage,
			[2] = "All"
		}

		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))

	end
	if Ctag.Text == "ðŸ› Developer" then
		local chatmessage = game.CoreGui.PartnerUI.Frame.TextBox.Text

		local args = {
			[1] = "[ðŸ› Developer]: " .. chatmessage,
			[2] = "All"
		}

		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))

	end

end)

