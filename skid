local PartnerUI = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local TextBox = Instance.new("TextBox")
local TextButton = Instance.new("TextButton")
local UICorner_2 = Instance.new("UICorner")
local on = Instance.new("TextButton")
local UICorner_3 = Instance.new("UICorner")
local off = Instance.new("TextButton")
local UICorner_4 = Instance.new("UICorner")
local TextLabel = Instance.new("TextLabel")
local TextLabel_2 = Instance.new("TextLabel")

PartnerUI.Name = "PartnerUI"
PartnerUI.Parent = game.CoreGui
PartnerUI.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = PartnerUI
Frame.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
Frame.Position = UDim2.new(0.273684204, 0, 0.150617287, 0)
Frame.Size = UDim2.new(0, 327, 0, 98)
Frame.Active = true
Frame.Draggable = true

UICorner.CornerRadius = UDim.new(0, 10)
UICorner.Parent = Frame

TextBox.Parent = Frame
TextBox.BackgroundColor3 = Color3.fromRGB(85, 85, 85)
TextBox.Position = UDim2.new(0.0244648326, 0, 0.095864661, 0)
TextBox.Size = UDim2.new(0, 238, 0, 24)
TextBox.Font = Enum.Font.SourceSans
TextBox.Text = ""
TextBox.TextColor3 = Color3.fromRGB(165, 165, 165)
TextBox.TextScaled = true
TextBox.TextSize = 14.000
TextBox.TextWrapped = true

TextButton.Parent = Frame
TextButton.BackgroundColor3 = Color3.fromRGB(75, 75, 75)
TextButton.Position = UDim2.new(0.773700297, 0, 0.0827067643, 0)
TextButton.Size = UDim2.new(0, 69, 0, 24)
TextButton.Font = Enum.Font.Gotham
TextButton.Text = "Send"
TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
TextButton.TextScaled = true
TextButton.TextSize = 14.000
TextButton.TextWrapped = true
TextButton.MouseButton1Down:connect(function()
	if on.Visible == true then
		local chatmessage = game.CoreGui.PartnerUI.Frame.TextBox.Text

		local args = {
			[1] = "[ðŸ”§Moderator]: " .. chatmessage,
			[2] = "All"
		}

		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
		
	else
		
		local chatmessage = game.CoreGui.PartnerUI.Frame.TextBox.Text

		local args = {
			[1] = "[ðŸ”¥Partner]: " .. chatmessage,
			[2] = "All"
		}

		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))

	end
		
end)

UICorner_2.Parent = TextButton

on.Name = "on"
on.Parent = Frame
on.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
