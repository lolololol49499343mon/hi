-- Scripts:

local function TQQQ_fake_script() -- on.LocalScript 
	local script = Instance.new('LocalScript', on)

	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Visible = false
		script.Parent.Parent.off.Visible = true
	end)
end
coroutine.wrap(TQQQ_fake_script)()
local function EOBSNB_fake_script() -- off.LocalScript 
	local script = Instance.new('LocalScript', off)

	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Visible = false
		script.Parent.Parent.on.Visible = true
	end)
end
coroutine.wrap(EOBSNB_fake_script)()
local function UZZKBK_fake_script() -- extraoption.LocalScript 
	local script = Instance.new('LocalScript', extraoption)

	script.Parent.MouseButton1Down:connect(function()
		game.CoreGui.PartnerUI.Frame.Frame.Visible = true
		game.CoreGui.PartnerUI.Frame.closeextra.Visible = true
		script.Parent.Visible = false
	end)
end
coroutine.wrap(UZZKBK_fake_script)()
local function EHNZTX_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	script.Parent.MouseButton1Down:connect(function()
		game:GetService('TeleportService'):Teleport(7722306047)
	end)
	
end
coroutine.wrap(EHNZTX_fake_script)()
